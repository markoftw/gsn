{
  "name": "@opengsn/contracts",
  "license": "GPL-3.0-only",
  "version": "3.0.0-beta.10",
  "main": "dist/types/typechain-types/index.js",
  "scripts": {
    "tsc": "tsc",
    "solidity-docgen": "rm -rf docs && solidity-docgen --solc-module solc-0.8 --input=src --exclude=src/test,src/forwarder/test --templates=.",
    "hardhat-compile": "hardhat compile",
    "lint": "solhint -f unix \"src/**/*.sol\" --max-warnings 0",
    "rm-dist": "rm -rf types/truffle-contracts"
  },
  "files": [
    "src/*",
    "dist/*",
    "types/*",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@account-abstraction/contracts": "^0.7.0",
    "@ethersproject/abi": "^5.7.0",
    "@openzeppelin/contracts": "^4.2.0",
    "ethers": "^6.11.1"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-ethers": "^3.0.5",
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "solhint": "^3.3.2",
    "solidity-docgen": "^0.5.16",
    "ts-node": "8.6.2",
    "typechain": "^8.3.2"
  }
}
